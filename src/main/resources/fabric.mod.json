{
	"schemaVersion": 1,
	"id": "qualitytools",
	"version": "${version}",
	"name": "QualityTools",
	"description": "This is a quality of life tool mod by DungeonDesk that adds several new tool materials as well as 3x3 Pickaxes and 3x3 Shovels.",
	"authors": [
		"DungeonDesk"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/qualitytools/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.dungeondesk.qualitytools.QualityTools"
		],
		"fabric-datagen": [
			"net.dungeondesk.qualitytools.QualityToolsDataGenerator"
		],
		"client": [
			"net.dungeondesk.qualitytools.QualityToolsClient"
		]
	},
	"mixins": [
		"qualitytools.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}